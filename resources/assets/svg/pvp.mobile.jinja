<svg width="770" height="300" viewBox="0 0 77 30" fill="none" xmlns="http://www.w3.org/2000/svg"
  xmlns:xlink="http://www.w3.org/1999/xlink">
  <defs>
    <rect id="rect1" width="40" height="20" rx="3" />
    <clipPath id="clip1">
      <use xlink:href="#rect1" />
    </clipPath>
    <rect id="rect2" x="-4" width="40" height="20" rx="3" />
    <clipPath id="clip2">
      <use xlink:href="#rect2" />
    </clipPath>
  </defs>
  <rect width="100" height="50" fill="#202020" />
  <g transform="translate(2,0)">
    <image x="0" y="0" width="8" height="8" href="img/rule/{{rule}}.svg" />
    <text x="8" y="6" font-size="4" fill="white">{{rule}}</text>
    <text text-anchor="end" x="72" y="6" font-size="3" fill="white">{{start_time}}</text>
  </g>
  <g transform="translate(2,7)">
    <g transform="translate(0,1)">
      <image width="36" height="20" preserveAspectRatio="xMidYMid slice"
        href="img/stage/vs/{{stages[0]}}.png"
        clip-path="url(#clip1)" />
    </g>
    <g transform="translate(37,1)">
      <image width="36" height="20" preserveAspectRatio="xMidYMid slice"
        href="img/stage/vs/{{stages[1]}}.png"
        clip-path="url(#clip2)" />
    </g>
    <g transform="translate(30.5,5)">
      <image width="12" height="12" href="img/mode/{{mode}}.svg" />
    </g>
  </g>
</svg>